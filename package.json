{
  "name": "ddsa-vue3-views",
  "version": "1.0.0",
  "type": "module",
  "main": "./dist/proformajs-vue3.umd.cjs",
  "module": "./dist/proformajs-vue3.js",
  "exports": {
    ".": {
      "import": "./dist/proformajs-vue3.js",
      "require": "./dist/proformajs-vue3.umd.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite dev",
    "build": "npm run build --workspaces && vite build",
    "preview": "vite build -c vite.preview.js && vite preview",
    "test:e2e": "start-server-and-test preview http://localhost:4173 'cypress run --e2e'",
    "test:e2e:dev": "start-server-and-test 'vite dev --port 4173' http://localhost:4173 'cypress open --e2e'",
    "test:unit": "cypress run --component",
    "test:unit:dev": "cypress open --component",
    "lint": "npm run lint --workspaces && eslint . --ext .vue,.js,.jsx,.cjs,.mjs --fix --ignore-path .gitignore",
    "format": "npm run format --workspaces && prettier --write src/ cypress/",
    "publishall": "node versionHelper.cjs && npm run lint && npm run build && npm publish && npm publish --workspace=perform"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.4.0",
    "@fortawesome/free-solid-svg-icons": "^6.4.0",
    "@fortawesome/vue-fontawesome": "^3.0.3",
    "@openclinical/proformajs": "^0.19.0",
    "bootstrap": "^5.2.3",
    "dompurify": "^3.0.2",
    "file-saver": "^2.0.5",
    "hammerjs": "^2.0.8",
    "marked": "^12.0.2",
    "moment": "^2.29.4",
    "vue": "^3.2.47",
    "vue3-ace-editor": "^2.2.2"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.2.0",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vue/eslint-config-prettier": "^7.1.0",
    "cypress": "^12.7.0",
    "eslint": "^8.34.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-vue": "^9.9.0",
    "prettier": "^2.8.4",
    "sass": "^1.62.0",
    "start-server-and-test": "^2.0.0",
    "vite": "^4.1.4",
    "vite-plugin-css-injected-by-js": "^3.1.0"
  },
  "workspaces": [
    "packages/tools",
    "packages/map",
    "packages/perform",
    "packages/compose"
  ]
}
